<div class="parent">
  <div class="child" id="left-col">
    <h1>Archive</h1>
  </div>
  <div class="child" id="right-col">
    <div class="text-right">
      <span><strong>({{page}} of {{pages}})</strong></span>
      <button class="btn btn-default btn-sm pager-btn" role="button" {{bind-attr disabled=atBeginning}} {{action 'prevPage'}}>
        <span class="glyphicon glyphicon-chevron-left"></span>
      </button>
      <button class="btn btn-default btn-sm pager-btn" role="button" {{bind-attr disabled=atEnd}} {{action 'nextPage'}}>
        <span class="glyphicon glyphicon-chevron-right"></span>
      </button>
    </div>
  </div>
</div>
<div class="list-group">
  {{#each}}
    {{#unless isNew}}
      {{#link-to "record" id class="list-group-item"}}
        <h4 class="list-group-item-heading">Name: {{name}}</h4>
        <p class="list-group-item-text">Runtime: {{format-date start}} to {{format-date stop}}</p>
      {{/link-to}}
    {{/unless}}
  {{/each}}
</div>